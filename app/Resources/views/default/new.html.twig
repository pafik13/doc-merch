{% extends 'default.html.twig' %}

{% block title %}Добавить представителя{% endblock %}

{% set page = 2 %}

{% block main %}
	{% form_theme form with 'bootstrap_3_layout.html.twig' %}
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header"> Добавить нового представителя </h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">Добавление представителя</div>
				<div class="panel-body">
					{{ form(form) }}
					<a class="btn btn-default btn-lg btn-block" href="{{ path('presenters') }}">Вернуться назад</a>
				</div>
			</div>
        </div>
        <!-- /.col-lg-12 -->
    </div>
	<!-- /.row -->
{% endblock %}